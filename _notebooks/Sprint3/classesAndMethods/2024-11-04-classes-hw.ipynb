        "    getTerrain(){\n",
        "        return this._terrain\n",
        "    }\n",
        "    //return details\n",
        "    displayDetails() {\n",
        "        return this\n",
        "    }\n",
        "    //compares ranges\n",
        "    static compareRange(aircraft1, aircraft2) {\n",
        "        return aircraft1.range - aircraft2.range;\n",
        "    }\n",
        "}\n",
